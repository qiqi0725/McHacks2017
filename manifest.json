{
  "manifest_version": 2,
  "name": "Snake Game",
  "description": "The classic Snake game, now playable whenever, wherever!",
  "version": "0.1",
  "commands": {
          "_execute_browser_action": {
            "suggested_key": {
              "default": "Ctrl+Shift+K",
              "mac": "Command+Shift+K"
            },
            "description": "Open game"
          }
   },
   "browser_action": {
  	 "default_icon": "Snake-icon.png",
  	 "default_popup": "index.html"
   },
   "icons": { "16": "Snake-icon.png",
           "48": "Snake-icon.png",
          "128": "Snake-icon.png" }


}